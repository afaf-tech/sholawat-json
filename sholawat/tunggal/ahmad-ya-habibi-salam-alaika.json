{
  "source": "terjemahkitab.com",
  "name": "أَحْمَدْ يَا حَبِيْبِيْ سَلَامْ عَلَيْكَ",
  "latin": "Ahmad Ya Habibi Salam Alaika",
  "text": {
    "1": {
      "arabic": "أَحْمَدْ يَا حَبِيْبِيْ سَلَامْ عَلَيْكَ",
      "latin": "Ahmad ya habibi salam alaika"
    },
    "2": {
      "arabic": "ياَ عَوْنَ الْغَرِيْبِ، سَلَامْ عَلَيْكَ",
      "latin": "Ya 'aunal ghoribi salam 'alaik"
    },
    "3": {
      "arabic": "أَمْنٌ وَسَلَامٌ، سَلَامْ عَلَيْكَ",
      "latin": "Amnun wa salaamun salam 'alaika"
    },
    "4": {
      "arabic": "دِيْنَكَ الْإِسْلَامُ، سَلَامْ عَلَيْكَ",
      "latin": "Dinukal islamu salam 'alaik"
    },
    "5": {
      "arabic": "جِئْتَ بِالْقُرْآنِ، سَلَامْ عَلَيْكَ",
      "latin": "Ji'ta bil qur'ani salam 'alaik"
    }
  },
  "translations": {
    "id": {
      "name": "Ahmad Wahai Kekasihku Salam Untukmu",
      "translator": "Terjemahkitab (Dr. Muhammad Ali Fathomi, Lc., M. H.)",
      "text": {
        "1": "Ahmad wahai kekasihku salam untukmu",
        "2": "Wahai penolong orang yang asing, salam untukmu",
        "3": "Keamanan dan keselamatan, salam untukmu",
        "4": "Agamamu islam, salam untukmu",
        "5": "Engkau datang dengan membawa Quran, salam untukmu"
      }
    }
  },
  "last_updated": "2025-11-21"
}
